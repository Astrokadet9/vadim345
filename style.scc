body {
    font-family: sans-serif;
    background-color: #00bfff;
}

h1 {
    text-align: center;
    color:rgb(0, 0, 0);
    opacity:2;
    
}
header h2 {
    color: #000000;
    text-align: center;
   
}
.head {
    height: 60px;
    width: 100%;
    background-color: aliceblue;
}
.navi {
    background-color: #ffffff;
    width: 100%;
    height: 130px;
    text-align: center;
   
}
a {
    margin: 20px;
}
a:hover {
    color: #00bfff;
}
.navigation {
    background-color: white;
    width: 100%;
    height: 600px;
    margin-top: 50px;
    display: flex;
   flex-wrap: wrap;
   background-image: none;
}
.kart {
    background-color: #ffa07a;
    width: 230px;
    height: 130px;
    margin-left: 50px;
    margin-right: 50px;
    margin-top: 50px;
   
}
.kart h3 {
    position: relative;
    bottom: 95px;
    text-align: end;
    background-color: #00bfff;

}
.kart h2 {
    text-align: center;
    position: relative;
    bottom: 0px;
}
.kart img {
    width: 230px;
    height: 200px;
    position: relative;
    bottom: 110px;
 
  
  
   
}
article {
    background-color: #ffa07a;
    height: 40px;
    position: relative;
    bottom: 145px;
   text-align: center;
  
}
h4:hover {
    color: rgb(255, 255, 255);
    font-size: 25px;

}
.mi {
    width: 100%;
    height: 300px;
    background-color: white;
    margin-top: 50px;
}
div p {
    text-align: center;


}
p:hover {
    color: #00bfff;
}

